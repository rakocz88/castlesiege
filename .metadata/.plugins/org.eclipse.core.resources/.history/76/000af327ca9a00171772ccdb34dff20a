package com.pilaf.cs.tests.builder;

public class UserTest {
	
	    private final String login; // required
	 
	    private UserTest(UserTestBuilder builder) {
	        this.login = builder.login;
	    }
	 
	    public String getLogin() {
	        return login;
	    }
	 
	 
	    public static class UserTestBuilder {
	        private final String login;
	 
	        public UserTestBuilder() {
	            
	        }
	        
	        public UserTestBuilder setLogin(String login){
	        	this.login = login;
	        }
	 
	 
	    }

}
